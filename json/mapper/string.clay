import json.model.*;
import json.mapper.*;


record StringJsonMapper[S] ();

[S]
overload JsonMapperMaps(static StringJsonMapper[S]) = S, JsonString;

[S | String?(S)]
overload JsonMapper(static S) = StringJsonMapper[S];

[S | String?(S)]
overload mapToJson(m: StringJsonMapper[S], s: S) = JsonString(s);

[S | String?(S)]
overload mapFromJson(m: StringJsonMapper[S], j: JsonString) = S(j.value);



